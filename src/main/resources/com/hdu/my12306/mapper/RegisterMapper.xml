<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.hdu.my12306.mapper.RegisterMapper">
    <select id="findByPhone" resultType="com.hdu.my12306.domain.User">
    select * from user where u_phone=#{phone};
    </select>
    <insert id="register">
        insert into user(u_gender, u_idcard, u_name, u_pwd, u_phone)
        VALUES(#{uGender},#{uIdcard},#{uName},#{uPwd},#{uPhone});

    </insert>
</mapper>